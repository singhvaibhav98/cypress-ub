{
  "name": "UBankAutomation",
  "version": "1.0.0",
  "devDependencies": {
    "@cypress/xpath": "^2.0.3",
    "cucumber": "^6.0.7",
    "cypress": "^13.6.6",
    "cypress-cucumber-preprocessor": "^4.3.1"
  },
  "dependencies": {
